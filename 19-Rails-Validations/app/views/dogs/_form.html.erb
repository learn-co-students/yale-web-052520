<%# flash[:first] %>
<% if flash[:errors] %>
    <ul>
    <% flash[:errors].each do |error| %>
        <li style="color: red">
            <%= error%>
        </li>
    <% end %>
    </ul>
<% end %>

<%= form_with model: @dog do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :breed %>
<%= f.text_field :breed %>

<%= f.label :age %>
<%= f.number_field :age %>

<%= f.submit %>

<% end %>